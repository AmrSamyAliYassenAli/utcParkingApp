<script>


    $("#areaN").autocomplete({
        source: function (request, response) {
            $.ajax({
                url: "/ParkingLocations/GrtArea/",
                data: { an: request.term },
                dataType: "json",

                //contentType: "application/json; charset=utf-8",
                success: function (data) {

                    
                        $("#hfarea").val(0);
                    
                    
                        response($.map(data, function (item) {
                            return item;
                        }))
                    
                    
                }
            });
        },
        select: function (e, i) {
            //if (data==null) {
            //    $("#hfarea").val(0);
            //}
           
            $("#hfarea").val(i.item.val);
        },
    });

</script>

                                                        <input name="IsMullti" type="checkbox" style="width:auto;" class="form-control inputPopUp" />
                                                    </div>*@

                        </div>
                    </form>
                </div>
                <div class="modal-footer">
                    <div class="col-xs-10">
                    </div>
                    <div class="col-xs-2">
                        <button type="submit" class="btn btn-success btn-save" id="btn_SavePopUpModal">Save</button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @{var enumerable = Model as System.Collections.IEnumerable;
        if (enumerable != null)
        {
            foreach (var item in Model)
            {
                <div id="@("a" + item.Id)" tabindex="-1" role="dialog" class="modal fade">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal">
                                    <span aria-hidden="true">×</span>
                                    <span class="sr-only">Close</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <div class="text-center">
                                    <span class="text-danger icon icon-times-circle icon-5x"></span>
                                    <h3 class="text-danger">Delete</h3>
                                    <p>Are You Sure You want to delete this Record ?</p>
                                    <div class="m-t-lg">
                                        <button class="btn btn-danger" data-dismiss="modal" type="button" id="@item.Id">OK</button>
                                        <button class="btn btn-default" data-dismiss="modal" type="button">Cancel</button>
                                    </div>
                                </div>
                            </div>
                            <div class="modal-footer"></div>
                        </div>
                    </div>
                </div>
            }
        }
    }

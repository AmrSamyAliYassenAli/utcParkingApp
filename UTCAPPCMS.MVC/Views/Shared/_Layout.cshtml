    @*################################################ Modal PopUp Form  ################################################*@
    <div id="modalGridSystemLg" tabindex="-1" role="dialog" class="modal fade">
        <div class="modal-dialog modal-lg">
            <div class="modal-content">

                <div class="modal-header bg-danger">
                    <button type="button" class="close" data-dismiss="modal">
                        <span aria-hidden="true">×</span>
                        <span class="sr-only">Close</span>
                    </button>
                    <h4 class="modal-title">Add New Subscription Duration Record , For this Subscription</h4>
                </div>

                <div class="modal-body">
                    <form class="form form-horizontal " id="PopUpForm">
                        <div class="form-group ">

                            <div class="col-xs-6">
                                <label class="control-label">durationInDays</label>
                                <input name="durationInDays" type="text" class="form-control inputPopUp title" required="required" />
                            </div>

                            <div class="col-xs-6">
                                <label class="control-label">DaysCount</label>
                                <input name="DaysCount" type="text" class="form-control inputPopUp title" />
                            </div>

                        </div>

                        <div class="form-group ">

                            <div class="col-xs-6">
                                <label class="control-label">HoursCount</label>
                                <input name="HoursCount" type="text" class="form-control inputPopUp title" />
                            </div>

                            <div class="col-xs-6">
                                <label class="control-label">VechicleCount</label>
                                <input name="VechicleCount" type="text" class="form-control inputPopUp title" />
                            </div>
                        </div>

                        <div class="form-group ">
                            <div class="col-xs-6">
                                <label class="control-label">Price</label>
                                <input name="Price" type="text" class="form-control inputPopUp title" />
                            </div>
                            <div class="col-xs-6">
                                <label class="control-label">PriceMulti</label>
                                <input name="PriceMulti" type="text" class="form-control inputPopUp title" />
                            </div>
                        </div>

                        <div class="form-group ">
                            <div class="col-xs-2">
                                <label class="control-label">AllDays</label><br />
                                <label class="switch switch-primary">
                                    <input class="form-control switch-input inputPopUp" name="AllDays" type="checkbox" id="chbx_AllDays">
                                    <span class="switch-track"></span>
                                    <span class="switch-thumb"></span>
                                </label>
                            </div>
                            <div class="col-xs-2">
                                <label class="control-label">AllTime</label><br />
                                <label class="switch switch-primary">
                                    <input class="form-control switch-input inputPopUp" name="AllTime" type="checkbox" id="chbx_AllTime">
                                    <span class="switch-track"></span>
                                    <span class="switch-thumb"></span>
                                </label>
                            </div>
                            <div class="col-xs-2">
                                <label class="control-label">IsMullti</label><br />
                                <label class="switch switch-primary">
                                    <input class="form-control switch-input inputPopUp" name="IsMullti" type="checkbox" id="chbx_IsMullti">
                                    <span class="switch-track"></span>
                                    <span class="switch-thumb"></span>
                                </label>
                            </div>
                            @*<div class="col-xs-2">
                                    <label class="control-label">AllDays</label>
                                    <input name="AllDays" type="checkbox" style="width:auto;" class="form-control inputPopUp" />
                                </div>

                                                    <div class="col-xs-2">
                                                        <label class="control-label">AllTime</label>
                                                        <input name="AllTime" type="checkbox" style="width:auto;" class="form-control inputPopUp" />
                                                    </div>

